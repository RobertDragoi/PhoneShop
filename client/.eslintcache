OGIN_SUCCES:\n        case REGISTER_SUCCES:\n            \n            localStorage.setItem('token',action.payload);\n            return {...state,token:action.payload,isAuthenticated:true}\n        case USER_LOADED:\n            console.log(action.payload.date)\n            return {...state,user:action.payload}\n        case LOGIN_FAIL:\n        case REGISTER_FAIL:\n            localStorage.removeItem('token');\n            return{\n                    ...state,\n                    token:null,\n                    isAuthenticated:false,\n                    user:null,\n                    error:action.payload \n            }\n        case CLEAR_ERRORS:\n                return{...state,error:null}\n        case LOGOUT:\n            localStorage.removeItem('token')\n                return{\n                    ...state,\n                    token:null,\n                    user:null,\n                    isAuthenticated:false\n                }\n        default:\n            return state\n    }\n}","C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Cart\\CartColumns.js",[],"C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Cart\\CartList.js",[],"C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Cart\\CartTotals.js",[],"C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Cart\\EmptyCart.js",[],"C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Modal.js",[],"C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Cart\\CartItem.js",["144"],"import React,{useContext} from 'react'\nimport ProductContext from '../ProductsState/productContext';\nexport default function CartItem(props) {\n    const productContext=useContext(ProductContext);\n    const {removeProduct,increment,decrement}=productContext;\n