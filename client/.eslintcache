Context;\n  const { user } = userContext;\n  const { id } = useParams();\n  console.log(loading);\n  useEffect(() => {\n    const fetchProductDetail = async (id) => {\n      await getProductDetail(id);\n    };\n    fetchProductDetail(id);\n  }, [id]);\n  return (\n    <div className=\"container\">\n      {loading ? (\n        <div class=\"d-flex justify-content-center\">\n          <div class=\"spinner-border\" role=\"status\">\n            <span class=\"sr-only\">Loading...</span>\n          </div>\n        </div>\n      ) : (\n        <div className=\"row\">\n          <div className=\"col-sm\">\n            <h2>{detail?.title}</h2>\n            <img\n              src={`${process.env.PUBLIC_URL}/${detail?.img}`}\n              alt=\"product\"\n              className=\"card-img-top\"\n            ></img>\n          </div>\n          <div className=\"col-sm\">\n            <div className=\"d-flex flex-column\">\n              <div style={{ padding: 40 }}>\n                <div className=\"p-2 bg-info\">Name: {detail?.title}</div>\n                <div className=\"p-2 bg-primary\">Company: {detail?.company}</div>\n                <div className=\"p-2 bg-warning\">Price: ${detail?.price}</div>\n                <div className=\"p-2 bg-info\">\n                  Description: {detail?.description}\n                </div>\n                {user ? (\n                  <ButtonContainer onClick={() => addProduct()}>\n                    Add to cart\n                  </ButtonContainer>\n                ) : (\n                  <Link to=\"/api/login\">\n                    <ButtonContainer>Log in </ButtonContainer>\n                  </Link>\n                )}\n              </div>\n            </div>\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default Details;\n","C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-Shop\\client\\src\\components\\Default.js",[],"C:\\Users\\ZZ02TH826\\Desktop\\Facultate\\LICENTA\\React.js-Phone-